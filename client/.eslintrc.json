{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "no-unused-vars":"off",
        "no-console":"off",
        "import/no-extraneous-dependencies":"off",
        "react/prop-types" : [0],
        "consistent-return":"off",
        "prefer-const":"off",
        "class-methods-use-this":"off",
        "react/jsx-no-bind": [1,{"allowFunctions":true,"allowArrowFunctions":true,"allowBind":true}]
    }
}
